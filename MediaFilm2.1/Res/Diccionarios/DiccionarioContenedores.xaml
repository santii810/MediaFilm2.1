<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:MediaFilm2._1.Diccionarios">

    <Style x:Key="BorderNegro" TargetType="{x:Type Border}">
        <Setter Property="BorderBrush" Value="Black"/>
        <Setter Property="BorderThickness" Value="4"/>
        <Setter Property="Background" Value="Bisque"/>
    </Style>
    <Style x:Key="PaginaPrincipal" TargetType="{x:Type Page}">
        <Setter Property="Margin" Value="156,0,0,0"/>
    </Style>
    <Style x:Key="BorderBisque" TargetType="{x:Type Border}">
        <Setter Property="BorderBrush" Value="Bisque"/>
        <Setter Property="BorderThickness" Value="3"/>
        <Setter Property="Margin" Value="2"/>
    </Style>
</ResourceDictionary>